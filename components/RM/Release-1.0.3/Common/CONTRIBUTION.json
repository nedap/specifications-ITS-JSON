{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://specifications.openehr.org/releases/ITS-JSON/latest/components/RM/Release-1.0.3/Common/CONTRIBUTION.json",
    "definitions": {
        "CONTRIBUTION": {
            "type": "object",
            "required": [
                "uid",
