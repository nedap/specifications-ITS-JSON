                                    "properties": {
                                        "_type": {
                                            "const": "OBJECT_REF"
                                        }
                                    },
                                    "required": [
                                        "_type"
                                    ]
                                },
                                "then": {
                                    "$ref": "https://specifications.openehr.org/releases/ITS-JSON/latest/components/RM/Release-1.0.3/Support/OBJECT_REF.json#/definitions/OBJECT_REF"
                                }
                            },
                            {
                                "if": {
                                    "not": {
                                        "required": [
                                            "_type"
                                        ]
                                    }
                                },
                                "then": {
