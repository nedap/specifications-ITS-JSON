    "properties": {
        "_type": {
            "enum": [
                "Time_Definitions",
                "Iso8601_date_time",
                "Iso8601_duration",
                "Iso8601_time",
                "Iso8601_timezone",
                "Iso8601_date",
                "DV_DATE_TIME",
                "DV_DURATION",
                "DV_TIME",
                "DV_DATE"
            ]
        }
    }
}