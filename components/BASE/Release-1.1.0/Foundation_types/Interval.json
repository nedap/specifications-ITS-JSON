{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://specifications.openehr.org/releases/ITS-JSON/latest/components/BASE/Release-1.1.0/Foundation_types/Interval.json",
    "definitions": {
        "INTERVAL": {
            "type": "object",
            "required": [
                "lower_unbounded",
                "upper_unbounded",
                "lower_included",
                "upper_included"
            ],
            "properties": {
                "lower": {
                    "type": "object"
                },
                "upper": {
                    "type": "object"
                },
                "lower_unbounded": {
                    "type": "boolean"
                },
                "upper_unbounded": {
                    "type": "boolean"
                },
                "lower_included": {
                    "type": "boolean"
                },
                "upper_included": {
                    "type": "boolean"
                },
                "_type": {
                    "type": "string",
                    "const": "INTERVAL"
                }
            },
            "description": "Type defining an interval of any ordered type.",
            "additionalProperties": false
        }
    }
}